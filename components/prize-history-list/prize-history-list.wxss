.prize-history-list {
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background-color: rgba(0,0,0,.5);
    z-index: 999;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-pack: center;
    justify-content: center;
    -ms-flex-align: center;
    align-items: center;
    flex-direction: column;
    perspective: 1000;
    transform-style: preserve-3d;
}

.dialog-close {
    width: 62rpx;
    height: 62rpx;
    margin-top: 20rpx;
    background-image: url(data:image/png;base64,);
    background-size: contain;
    background-position: bottom;
    background-repeat: no-repeat;
}

.prize-history-list-swiper {
    width: 100%;
    height: 1090rpx;
}

.prize-history-list-swiper .swiper-block,.prize-history-list-swiper .swiper-block .slide-cover {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center;
}

.prize-history-list-swiper .swiper-block {
    flex-direction: column;
    overflow: visible;
}

.prize-history-list-swiper .swiper-block .slide-cover {
    width: 625rpx;
    height: 100%;
}